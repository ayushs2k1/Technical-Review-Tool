<%- include("partials/header.ejs") %>
<link rel="stylesheet" href="/styles/form2.css">
<div class="form2-header">
    <h3><%= review.name %></h3>
    <h5><%= review.createdAt %> </h5>
</div>
<p style="margin-left:100px;"><strong>Objective</strong></p>
<div id="objective-box">
    <%= review.objective %>
</div>
<p style="margin-left:100px;"><strong>Comments</strong></p>
<div class="reviewForm2-container">
    <form action="/reviewForm/<%= review._id %>" method="POST">
        <div class="form2-col1">
            <label for="panelMember1"><strong>Panel Member 1</strong></label><br>
            <input type="text" name="comments" placeholder="Comment" id="panelMember1">
            <input type="number" name="severity" placeholder="Severity Level"><br>
            <label for="panelMember2"><strong>Panel Member 2</strong></label><br>
            <input type="text" name="comments" placeholder="Comment" id="panelMember2">
            <input type="number" name="severity" placeholder="Severity Level"><br>
            <label for="panelMember3"><strong>Panel Member 3</strong></label><br>
            <input type="text" name="comments" placeholder="Comment" id="panelMember3">
            <input type="number" name="severity" placeholder="Severity Level"><br>
        </div>
        <div class="form2-col2">
            <label for="panelMember4"><strong>Panel Member 4</strong></label><br>
            <input type="text" name="comments" placeholder="Comment" id="panelMember4">
            <input type="number" name="severity" placeholder="Severity Level"><br>
            <label for="panelMember5"><strong>Panel Member 5</strong></label><br>
            <input type="text" name="comments" placeholder="Comment" id="panelMember5">
            <input type="number" name="severity" placeholder="Severity Level"><br><br>
            <button class = "btn btn-success">Next</button>
        </div>
        
    </form>
</div>






<%- include("partials/footer.ejs") %>